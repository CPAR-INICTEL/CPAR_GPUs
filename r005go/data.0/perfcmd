perf record -v --control=fd:24,27 -o output.perf -N -B -T --sample-cpu -d -a --call-graph dwarf,1024 --user-regs=ip,bp,sp --compression-level=1 --threads --clockid=CLOCK_MONOTONIC_RAW -e cpu/period=0x325aa0,event=0x3c,name=\'CPU_CLK_UNHALTED.THREAD\'/Dukpp,cpu/period=0x325aa0,umask=0x3,name=\'CPU_CLK_UNHALTED.REF_TSC\'/Dukpp,cpu/period=0x325aa0,event=0xc0,name=\'INST_RETIRED.ANY\'/Dukpp,cpu/period=0x325aa0,event=0xec,umask=0x2,name=\'CPU_CLK_UNHALTED.DISTRIBUTED\'/ukpp 
